package com.entity;

public class Employee {
	private int empId;
	private String name;
	private int age;
	private int deptNo;
	
	public int getEmpId() {
		return empId;
	}
	public void setEmpId(int empID) {
		this.empId = empId;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public int getAge() {
		return age;
	}
	public void setAge(int age) {
		this.age = age;
	}
	public int getDeptNo() {
		return deptNo;
	}
	public void setDeptNo(int deptNo) {
		this.deptNo = deptNo;
	}

}
