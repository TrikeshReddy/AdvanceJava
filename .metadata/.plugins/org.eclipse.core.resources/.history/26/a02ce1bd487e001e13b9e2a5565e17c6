package com.entity;

public class Employee {
	private int empID;
	private String name;
	private int age;
	private int deptNo;
	public void setEmpID(int empID) {
		this.empID=empID;
	}
	public int getEmpID() {
		return empID;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public int getAge() {
		return age;
	}
	public void setAge(int age) {
		this.age = age;
	}
	public int getDeptNo() {
		return deptNo;
	}
	public void setDeptNo(int deptNo) {
		this.deptNo = deptNo;
	}

}
